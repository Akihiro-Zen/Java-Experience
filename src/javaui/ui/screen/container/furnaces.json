{
  "namespace": "furnaces_mcjava",

  "furnace_screen_content_panel@container_mcjava.container_panel": {
    "$rsrc_location": "assets/minecraft/textures/gui/container/furnace",
    "$container_bg": "resources_mcjava.furnace_bg",
    "$specified_content": "furnaces_mcjava.furnace_content_panel"
  },

  "blast_furnace_screen_content_panel@redstone_mcjava.dispenser_screen_content_panel": {
    "$rsrc_location": "assets/minecraft/textures/gui/container/blast_furnace",
    "$container_bg": "resources_mcjava.blast_furnace_bg",
    "$specified_content": "furnaces_mcjava.blast_furnace_content_panel"
  },

  "smoker_screen_content_panel@redstone_mcjava.dispenser_screen_content_panel": {
    "$rsrc_location": "assets/minecraft/textures/gui/container/smoker",
    "$container_bg": "resources_mcjava.smoker_bg",
    "$specified_content": "furnaces_mcjava.smoker_content_panel"
  },

  "furnace_content_panel": {
    "type": "panel",
    "controls": [
      {
        "title@container_mcjava.title": {
          "offset": [0, 5],
          "anchor_from": "top_middle",
          "anchor_to": "top_middle"
        }
      },
      {
        "arrow@resources_mcjava.furnace_full_arrow": {
          "anchor_from": "top_left",
          "anchor_to": "top_left",
          "offset": [79, 34]
        }
      },
      {
        "flame@resources_mcjava.furnace_full_flame": {
          "anchor_from": "top_left",
          "anchor_to": "top_left",
          "offset": [56, 36]
        }
      },
      { "ingredient@furnaces_mcjava.ingredient_item": {} },
      { "fuel@furnaces_mcjava.fuel_item": {} },
      { "output@furnaces_mcjava.output_item": {} }
    ]
  },

  "furnace_item_slot@container_mcjava.container_slot": {
    "anchor_from": "top_left",
    "anchor_to": "top_left"
  },
  "ingredient_item@furnaces_mcjava.furnace_item_slot": {
    "offset": [55, 16],
    "$item_collection_name": "furnace_ingredient_items",
    "$focus_id": "furnace_ingredient_item",
    "$focus_override_left": "FOCUS_OVERRIDE_STOP",
    "$focus_override_left_binding_type": "none",
    "$focus_override_up": "FOCUS_OVERRIDE_STOP",
    "$focus_override_up_binding_type": "none"
  },

  "fuel_item@furnaces_mcjava.furnace_item_slot": {
    "offset": [55, 52],
    "$item_collection_name": "furnace_fuel_items",
    "$focus_id": "furnace_fuel_item",
    "$focus_override_right": "furnace_output_item",
    "$focus_override_right_binding_type": "none",
    "$focus_override_left": "FOCUS_OVERRIDE_STOP",
    "$focus_override_left_binding_type": "none"
  },

  "output_item@furnaces_mcjava.furnace_item_slot": {
    "offset": [115, 34],
    "$item_collection_name": "furnace_output_items",
    "$focus_id": "furnace_output_item",
    "$focus_override_right_binding_type": "none",
    "$focus_override_right": "FOCUS_OVERRIDE_STOP",
    "$focus_override_left_binding_type": "none",
    "$focus_override_left": "furnace_fuel_item"
  }
}