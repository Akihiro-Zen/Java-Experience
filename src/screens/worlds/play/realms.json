    "$bg_unchecked_hover_src": "realms_mcjava.close_realms_info_button_hover",
    "$bg_checked_hover_src": "realms_mcjava.close_realms_info_button_hover",
    "$bg_locked_src": "realms_mcjava.close_realms_info_button_default",
    "$toggle_button_content": "realms_mcjava.with_alpha_hover_text"
  },

  "close_realms_info_button_bg@toggle_mcjava.toggle_button": {
    "$cancel_mapping": true,
