{
  "namespace": "realms_mcjava",

  "realms_screen_content_panel": {
    "type": "panel",
    "controls": [
      { "realms_content_panel@realms_content_panel": { "ignored": "$pre_release" } },
      { "realms_content_panel@beta_realms_content_panel": { "ignored": "(not $pre_release)" } },
      { "popup_dialog_factory@popup_dialog_factory": {} },
      { "realms_help_content_panel@realms_mcjava.realms_help_content_panel": { "ignored": "$pre_release" } }
    ],
    "bindings": [
      {
        "binding_type": "view",
        "source_control_name": "realms_view",
        "source_property_name": "#toggle_state",
        "target_property_name": "#visible"
      }
    ]
  },

  "realms_content_panel@common_mcjava.scroll_screen": {
    "$top_content": "realms_mcjava.top_content",
    "$scrolling_content": "realms_mcjava.scroll_content",
    "$bottom_content": "realms_mcjava.bottom_content",
    "$bottom_content_size": ["100%", 40],
    /* Reset Clip */
    "$middle_clip_path": false,
    "$scroll_clip_path": false
  },

  "beta_realms_content_panel": {
    "type": "panel",
    "controls": [
      { "bg@common_mcjava.dirt_backgound": {} },
      {
        "centered": {
          "type": "stack_panel",
          "size": ["100%", 138],
          "anchor_from": "center",
          "anchor_to": "top_middle",
          "offset": [0, "-30%"],
          "controls": [
            {
              "@common_mcjava.panel": {
                "size": ["100%", 10],
                "controls": [
                  {
                    "title": {
                      "type": "label",
                      "color": "red",
                      "anchor_from": "center",
                      "anchor_to": "center",
                      "shadow": true,
                      "text": "mco.client.incompatible.title",
                      "text_alignment": "center"
                    }
                  }
                ]
              }
            },
            { "space_zero@common_mcjava.vspace": { "size": [0, 18] } },
            {
              "@common_mcjava.panel": {
                "size": ["100%", 10],
                "controls": [
                  {
                    "title": {
                      "type": "label",
                      "shadow": true,
                      "anchor_from": "center",
                      "anchor_to": "center",
                      "text": "mco.client.incompatible.msg.line1",
                      "text_alignment": "center"
                    }
                  }
                ]
              }
            },
            {
              "@common_mcjava.panel": {
                "size": ["100%", 11],
                "controls": [
                  {
                    "title": {
                      "type": "label",
                      "shadow": true,
                      "anchor_from": "center",
                      "anchor_to": "center",
                      "text": "mco.client.incompatible.msg.line2",
                      "text_alignment": "center"
                    }
                  }
                ]
              }
            },
            {
              "@common_mcjava.panel": {
                "size": ["100%", 12],
                "controls": [
                  {
                    "title": {
                      "type": "label",
                      "shadow": true,
                      "anchor_from": "center",
                      "anchor_to": "center",
                      "text": "mco.client.incompatible.msg.line3",
                      "text_alignment": "center"
                    }
                  }
                ]
              }
            },
            { "space_zero@common_mcjava.vspace": { "size": [0, 58] } },
            {
              "@common_mcjava.panel": {
                "size": ["100%", 20],
                "controls": [
                  {
                    "left@play_mcjava.left_button": {
                      "$toggle_button_name": "realms",
                      "anchor_from": "center",
                      "anchor_to": "center",
                      "offset": [0, 0],
                      "$toggle_button_text": "gui.ok",
                      "size": [200, 20],
                      "$toggle_control_name": ""
                    }
                  }
                ]
              }
            }
          ]
        }
      }
    ]
  },

  "top_content": {
    "type": "panel",
    "controls": [
      { "left@play_mcjava.left_button": { "$toggle_button_name": "realms", "$toggle_control_name": "" } },
      { "content_panel@realms_mcjava.realms_title_panel": {} },
      { "content_panel@realms_mcjava.notification_button_panel": {} },
      { "content_panel@realms_mcjava.realms_info_button": {} }
    ]
  },

  "realms_title_panel": {
    "type": "panel",
    "size": [101, 29],
    "anchor_from": "top_middle",
    "anchor_to": "top_middle",
    "offset": [0, 3],
    "layer": 1,
    "controls": [
      {
        "title": {
          "type": "image",
          "texture": "assets/realms/textures/gui/title/realms",
          "uv": [0, 0],
          "uv_size": [199, 48],
          "size": ["100%", "100%"]
        }
      }
    ]
  },

  "scroll_content": {
    "type": "stack_panel",
    "size": [ "100%", "100%c" ],
    "layer": 1,
    "controls": [
      { "space_top": { "type": "panel", "size": [0, 4] } },
      {
        "friends_realms": {
          "type": "stack_panel",
          "size": [ "100%", "100%c" ],
          "clips_children": true,
          "controls": [
            { "friends_realm_selection@realms_mcjava.friends_realm_selection_panel": {} },
            { "friends_realms_grid@realms_mcjava.friends_realms_grid": {} }
          ],
          "bindings": [
            {
              "binding_name": "#realms_grids_visible",
              "binding_name_override": "#visible"
            }
          ]
        }
      }
    ]
  },

  "bottom_content": {
    "type": "panel",
    "anchor_from": "bottom_middle",
    "anchor_to": "bottom_middle",
    "size": [ 310, 20],
    "offset": [0, -10],
    "controls": [
      { "@realms_mcjava.friends_realm_join_panel": {} },
      { "@realms_mcjava.friends_realm_feed_panel": {} },
      { "@realms_mcjava.feed_selected_realm_button_locked": {} },
      { "@realms_mcjava.play_selected_realm_button_locked": {} },
      { "@realms_mcjava.cancel_button": {} },
      { "@realms_mcjava.join_by_code_button_locked": {} },
      { "@realms_mcjava.join_by_code_button": {} }
    ]
  },

  /* Content */

  "popup_dialog_factory": {
    "type": "factory",
    "control_ids": {
      "join_by_code": "@realms_mcjava.join_realm_modal"
    }
  },

  "join_realm_modal@popup_dialog.modal_input_panel": {
    "controls": [
      { "join_realm@join_realm_content": {} },
      {
        "dirt_background@common_mcjava.dirt_background": {
          "layer": -1
        }
      },
      {
        "gamepad_helpers@common.gamepad_helpers_a_and_b": {
          "layer": 10,
          "anchor_from": "bottom_middle",
          "anchor_to": "bottom_middle",
          "offset": [ 0, "31px" ]
        }
      }
    ]
  },

  "join_realm_content": {
    "type": "panel",
    "controls": [
      { "title@common_mcjava.title": { "text": "Join By Code" } },
      {
        "content_panel": {
          "type": "stack_panel",
          "anchor_from": "center",
          "anchor_to": "top_middle",
          "size": [ 200, 195],
          "offset": [ 0, "-38%" ],
          "controls": [
            { "space_zero": { "type": "panel", "size": [0, 4] } },
            {
              "panel_one": {
                "type": "panel",
                "size": [ 200, 22],
                "controls": [
                  {
                    "join_by_code_popup_code_text_field@text_input_mcjava.text_input": {
                      "$text_input_name": "#join_by_code_text_edit",
                      "max_length": 11,
                      "$placeholder_text": "networkWorld.realmsHashPlaceholder"
                    }
                  }
                ]
              }
            },
            { "space_bottom": { "type": "panel", "size": [0, 32] } },
            {
              "join_by_code_popup_join_button@button_mcjava.button": {
                "size": [200, 20],
                "layer": 1,
                "$button_text": "Join Realm",
                "$pressed_button_name": "button.menu_network_join_by_code_popup_join",
                "button_mappings": [
                  {
                    "from_button_id": "button.menu_select",
                    "to_button_id": "$pressed_button_name",
                    "mapping_type": "pressed"
                  },
                  {
                    "from_button_id": "button.menu_ok",
                    "to_button_id": "$pressed_button_name",
                    "mapping_type": "focused"
                  }
                ]
              }            
            },
            { "space_bottom": { "type": "panel", "size": [0, 4] } },
            {
              "close_button@button_mcjava.button": {
                "size": [200, 20],
                "$button_text": "gui.cancel",
                "button_mappings": [
                  {
                    "from_button_id": "button.menu_select",
                    "to_button_id": "button.menu_network_join_by_code_close",
                    "mapping_type": "pressed"
                  },
                  {
                    "from_button_id": "button.menu_ok",
                    "to_button_id": "button.menu_network_join_by_code_close",
                    "mapping_type": "focused"
                  }
                ]
              }
            },
            { "space_bottom": { "type": "panel", "size": [0, "fill"] } }
          ]
        }
      }
    ]
  },

  /* Notification / Invites */
  
  "notification_button_panel": {
    "type": "panel",
    "size": [ 20, 20 ],
    "anchor_from": "top_middle",
    "anchor_to": "top_middle",
    "offset": [60, 6],
    "controls": [
      {
        "notification_button@common.button": {
          "size": [20, 20],
          "$pressed_button_name": "button.menu_invite_notification",
          "$invite_texture": "textures/ui/invite_base",
          "controls": [
            {
              "image@play.invite_notification_icon": {
                "layer": 4
              }
            },
            { "hover@realms_mcjava.notification_hover": {} },
            { "pressed@realms_mcjava.notification_hover": {} }
          ]
        }
      }
    ]/* ,
    "bindings": [
      {
        "binding_name": "#realms_notification_button_visible",
        "binding_name_override": "#visible",
        "binding_type": "global"
      }
    ] */
  },

  "notification_hover": {
    "type": "panel",
    "size": [ 20, 20 ],
    "layer": 2,
    "controls": [
      {
        "@common_mcjava.alpha_hover_text": {
          "$hover_text": "mco.invites.title",
          "layer": 10,
          "bindings": [
            {
              "binding_name": "#realms_notification_button_visible",
              "binding_name_override": "#visible",
              "binding_type": "global"
            }
          ]
        } 
      },
      {
        "@common_mcjava.alpha_hover_text": {
          "$hover_text": "mco.invites.nopending",
          "layer": 10,
          "bindings": [
            {
              "binding_name": "(not #realms_notification_button_visible)",
              "binding_name_override": "#visible",
              "binding_type": "global"
            }
          ]
        }
      },
      {
        "background": {
          "type": "image",
          "texture": "src/assets/img/white",
          "color": [0.2, 0.2, 0.2],
          "size": [20, 20],
          "layer": 1
        }
      },
      {
        "background_anim": {
          "type": "image",
          "texture": "src/assets/img/white",
          "color": [0.1, 0.1, 0.1],
          "size": [20, 20],
          "alpha": "@realms_mcjava.anim_notification_bg_alpha_0",
          "layer": 1
        }
      },
      {
        "border": {
          "type": "image",
          "texture": "src/assets/img/border",
          "size": [20, 20],
          "layer": 1
        }
      },
      {
        "border_anim": {
          "type": "image",
          "texture": "src/assets/img/border",
          "color": [0.5, 0.5, 0.5],
          "size": [20, 20],
          "alpha": "@realms_mcjava.anim_notification_border_alpha_0",
          "layer": 1
        }
      }
    ]
  },

  "anim_notification_bg_alpha_0": {
    "anim_type": "alpha",
    "from": 0,
    "to": 1,
    "duration": 0.50,
    "next": "@realms_mcjava.anim_notification_bg_alpha_1"
  },

  "anim_notification_bg_alpha_1": {
    "anim_type": "alpha",
    "from": 1,
    "to": 0,
    "duration": 0.50,
    "next": "@realms_mcjava.anim_notification_bg_alpha_0"
  },

  "anim_notification_border_alpha_0": {
    "anim_type": "alpha",
    "from": 0,
    "to": 1,
    "duration": 0.50,
    "next": "@realms_mcjava.anim_notification_border_alpha_1"
  },

  "anim_notification_border_alpha_1": {
    "anim_type": "alpha",
    "from": 1,
    "to": 0,
    "duration": 0.50,
    "next": "@realms_mcjava.anim_notification_border_alpha_0"
  },

  /* Realms Help/Info */
  
  "realms_info_button@toggle_mcjava.toggle_button": {
    "$texture_src": "assets/realms/textures/gui/realms/questionmark",
    "size": [20, 20],
    "$hover_text": "mco.selectServer.info",
    "$toggle_index": 1,
    "$button_text": "",
    "anchor_from": "top_right",
    "anchor_to": "top_right",
    "$toggle_button_name": "realms_info",
    "$toggle_control_name": "realms_info_view", 
    "offset": [-17, 6],
    "$bg_unchecked_src": "realms_mcjava.realms_info_button_default",
    "$bg_checked_src": "realms_mcjava.realms_info_button_default",
    "$bg_unchecked_hover_src": "realms_mcjava.realms_info_button_hover",
    "$bg_checked_hover_src": "realms_mcjava.realms_info_button_hover",
    "$bg_locked_src": "realms_mcjava.realms_info_button_default",
    "$button_content": "realms_mcjava.with_alpha_hover_text"
  },

  "realms_info_button_default@common_mcjava.image": {
    "size": [20, 20],
    "variables": [
      { "requires": "($realms_resolution = 256x)", "$0": [0, 0], "$1": [20, 20] },
      { "requires": "($realms_resolution = 512x)", "$0": [0, 0], "$1": [40, 40] },
      { "requires": "($realms_resolution = 1024x)", "$0": [0, 0], "$1": [80, 80] },
      { "requires": "($realms_resolution = 2048x)", "$0": [0, 0], "$1": [160, 160] },
      { "requires": "($realms_resolution = 4096x)", "$0": [0, 0], "$1": [320, 320] }
    ]
  },

  "realms_info_button_hover@common_mcjava.image": {
    "size": [20, 20],
    "uv": "$2",
    "uv_size": "$3",
    "variables": [
      { "requires": "($realms_resolution = 256x)", "$2": [20, 0], "$3": [20, 20] },
      { "requires": "($realms_resolution = 512x)", "$2": [40, 0], "$3": [40, 40] },
      { "requires": "($realms_resolution = 1024x)", "$2": [80, 0], "$3": [80, 80] },
      { "requires": "($realms_resolution = 2048x)", "$2": [160, 0], "$3": [160, 160] },
      { "requires": "($realms_resolution = 4096x)", "$2": [320, 0], "$3": [320, 320] }
    ]
  },

  "with_alpha_hover_text": {
    "type": "panel",
    "controls": [
      {
        "alpha_hover_text@common_mcjava.alpha_hover_text": {
          "visible": "($unchecked_hover_state or $checked_hover_state)"
        }
      }
    ]
  },

  "close_realms_info_button@toggle_mcjava.toggle_button": {
    "$hover_text": "mco.selectServer.close",
    "$cancel_mapping": true,
    "size": [12, 12],
    "$focus_index": 0,
    "$texture_src": "assets/realms/textures/gui/realms/cross_icon",
    "$toggle_button_name": "realms_info",
    "$toggle_control_name": "not_realms_info_view", 
    "anchor_from": "top_left",
