{
  "namespace": "multiplayer_mcjava",

  "multiplayer_screen_content_panel@common_mcjava.scroll_screen": {
    "$top_content": "multiplayer_mcjava.top_content",
    "$scrolling_content": "multiplayer_mcjava.scroll_content",
    "$bottom_content": "multiplayer_mcjava.bottom_content",
    "$bottom_content_size": ["100%", 64],
    "$popup_dialog_factory": "multiplayer_mcjava.popup_dialog_factory",
    /* Reset Clip */
    "$middle_clip_path": false,
    "$scroll_clip_path": false,
    "bindings": [
      {
        "binding_type": "view",
        "source_control_name": "realms_view",
        "source_property_name": "(not #toggle_state)",
        "target_property_name": "#visible"
      }
    ]
  },

  "top_content": {
    "type": "panel",
    "controls": [
      { "title@common_mcjava.title": { "text": "multiplayer.title", "offset": [0, 20] } },
      { "left@play_mcjava.left_button": {  "$toggle_button_name": "multiplayer", "$toggle_control_name": "" } },
      { "right@play_mcjava.right_button": { "$toggle_button_name": "realms", "$toggle_control_name": "realms_view" } }
    ]
  },

  "scroll_content": {
    "type": "stack_panel",
    "size": [ "100%", "100%c" ],
    "layer": 1,
    "controls": [
      { "space_top@common_mcjava.vspace": {} },
      {
        "lan_servers": {
          "type": "stack_panel",
          "size": [ "100%", "100%c" ],
          "clips_children": true,
          "controls": [
            { "lan_servers_selection@multiplayer_mcjava.lan_server_selection_panel": {} },
            { "lan_servers_grid@multiplayer_mcjava.lan_servers_grid": {} }
          ]
        }
      },
      {
        "friends_servers": {
          "type": "stack_panel",
          "size": [ "100%", "100%c" ],
          "clips_children": true,
          "controls": [
            { "friends_servers_selection@multiplayer_mcjava.friends_server_selection_panel": {} },
            { "friends_servers_grid@multiplayer_mcjava.friends_servers_grid": {} }
          ]
        }
      },
      {
        "cross_platform_servers": {
          "type": "stack_panel",
          "size": [ "100%", "100%c" ],
          "clips_children": true,
          "controls": [
            { "cross_platform_servers_selection@multiplayer_mcjava.cross_platform_server_selection_panel": {} },
            { "cross_platform_servers_grid@multiplayer_mcjava.cross_platform_servers_grid": {} }
          ]
        }
      },
      {
        "featured_servers": {
          "type": "stack_panel",
          "size": [ "100%", "100%c" ],
          "clips_children": true,
          "controls": [
            { "featured_servers_selection@multiplayer_mcjava.featured_server_selection_panel": {} },
            { "featured_servers_grid@multiplayer_mcjava.featured_servers_grid": {} }
          ],
          "bindings": [
            {
              "binding_name": "#featured_servers_visible",
              "binding_name_override": "#visible"
            }
          ]
        }
      },
      {
        "more_servers": {
          "type": "stack_panel",
          "size": [ "100%", "100%c" ],
          "clips_children": true,
          "controls": [
            { "more_servers_selection@multiplayer_mcjava.more_server_selection_panel": {} },
            { "more_servers_grid@multiplayer_mcjava.more_servers_grid": {} }
          ]
        }
      },
      { "scanning_for_servers@multiplayer_mcjava.scanning_for_servers_panel": {} },
      { "space_top@common_mcjava.vspace": {} }
    ]
  },

  "bottom_content": {
    "type": "panel",
    "anchor_from": "bottom_middle",
    "anchor_to": "bottom_middle",
    "size": [ 310, 44],
    "offset": [0, -8],
    "controls": [
      { "featured_server_join@multiplayer_mcjava.featured_server_join_panel": {} },
      { "more_server_join@multiplayer_mcjava.more_server_join_panel": {} },
      { "more_server_edit@multiplayer_mcjava.more_server_edit_panel": {} },
      { "join_selected_server_button_locked@multiplayer_mcjava.join_selected_server_button_locked": {} },
      { "edit_selected_server_button_locked@multiplayer_mcjava.edit_selected_server_button_locked": {} },
      { "add_friend_button@multiplayer_mcjava.add_friend_button": {} },
      { "add_friend_button_locked@multiplayer_mcjava.add_friend_button_locked": {} },
      { "add_server_button@multiplayer_mcjava.add_server_button": {} },
      { "cancel_button@singleplayer_mcjava.cancel_button": { "size": [150, 20] } }
    ]
  },

  /* Content */

  "popup_dialog_factory": {
    "type": "factory",
    "control_ids": {
      "join_by_code": "@realms_content_panel.fail_account_modal"
    }
  },

  "featured_servers_grid@play_mcjava.grid_template": {
    "$for_src": "assets/minecraft/textures/gui/server_selection",
    "$grid_dimension_binding": "#third_party_featured_item_grid_dimension",
    "$collection_name": "third_party_server_network_worlds",
    "$grid_name": "featured_servers",
    "$from": "multiplayer"
  },

  "more_servers_grid@play_mcjava.grid_template": {
    "$for_src": "assets/minecraft/textures/gui/server_selection",
    "$grid_dimension_binding": "#servers_network_world_item_grid_dimension",
    "$collection_name": "servers_network_worlds",
    "$grid_name": "more_servers",
    "$from": "multiplayer"
  },

  "lan_servers_grid@play_mcjava.grid_template": {
    "$for_src": "assets/minecraft/textures/gui/server_selection",
    "$grid_dimension_binding": "#lan_network_world_item_grid_dimension",
    "$collection_name": "lan_network_worlds",
    "$grid_name": "lan_servers",
    "$from": "multiplayer"
  },

  "friends_servers_grid@play_mcjava.grid_template": {
    "$for_src": "assets/minecraft/textures/gui/server_selection",
    "$grid_dimension_binding": "#friends_network_world_item_grid_dimension",
    "$collection_name": "friends_network_worlds",
    "$grid_name": "friends_servers",
    "$from": "multiplayer"
  },

  "cross_platform_servers_grid@play_mcjava.grid_template": {
    "$for_src": "assets/minecraft/textures/gui/server_selection",
    "$grid_dimension_binding": "#cross_platform_friends_network_world_item_grid_dimension",
    "$collection_name": "cross_platform_friends_network_worlds",
    "$grid_name": "cross_platform_servers",
    "$from": "multiplayer"
  },

  /* Server Icon Images */

  "featured_servers_icon_image@multiplayer_mcjava.third_party_server_screenshot": {
    "layer": 2,
    "bindings": [
      {
        "binding_name": "#third_party_screenshot_visible",
        "binding_name_override": "#visible",
        "binding_type": "collection",
        "binding_collection_name": "$collection_name"
      }
    ]
  },

  "more_servers_icon_image": {
    "type": "image",
    "texture": "assets/minecraft/textures/misc/unknown_server",
    "size": [32, 32],
    "anchor_from": "top_left",
    "anchor_to": "top_left",
    "layer": 3
  },

  "lan_servers_icon_image@more_servers_icon_image": {
  },

  "friends_servers_icon_image": {
    "type": "image",
    "size": [32, 32],
    "anchor_from": "top_left",
    "anchor_to": "top_left",
    "layer": 3,
    "fill": true,
    "bindings": [
      {
        "binding_name": "#friends_server_icon_texture_name",
        "binding_name_override": "#texture",
        "binding_type": "collection",
        "binding_collection_name": "friends_network_worlds"
      }
    ]
  },

  "cross_platform_servers_icon_image": {
    "type": "image",
    "size": [32, 32],
    "anchor_from": "top_left",
    "anchor_to": "top_left",
    "layer": 3,
    "fill": true,
    "bindings": [
      {
        "binding_name": "#cross_platform_friends_server_icon_texture_name",
        "binding_name_override": "#texture",
        "binding_type": "collection",
        "binding_collection_name": "cross_platform_friends_network_worlds"
      }
    ]
  },

  "third_party_server_screenshot": {
    "type": "panel",
    "size": [32, 32],
    "anchor_from": "top_left",
    "anchor_to": "top_left",
    "layer": 3,
    "controls": [
      {
        "picture": {
          "type": "image",
          "layer": 1,
          "size": [32, 32],
          "fill": true,
          "anchor_from": "top_left",
          "anchor_to": "top_left",
          "bindings": [
            {
              "binding_name": "#third_party_server_logo_texture_path",
              "binding_type": "collection",
              "binding_name_override": "#texture",
              "binding_collection_name": "$collection_name"
            },
            {
              "binding_name": "#third_party_server_logo_resource_location",
              "binding_type": "collection",
              "binding_name_override": "#texture_file_system",
              "binding_collection_name": "$collection_name"
            }
          ]
        }
      }
    ]
  },

  /* Server Buttons */

  "featured_servers_button@play_mcjava.grid_button": {
    "$pressed_button_name": "button.connect_to_third_party_server",
    "bindings": [
      {
        "binding_type": "collection_details",
        "binding_collection_name": "$collection_name"
      },
      {
        "binding_name": "#cross_platform_enabled",
        "binding_name_override": "#enabled"
      }
    ],
    "$button_text": "selectServer.select"
  },

  "more_servers_button@play_mcjava.grid_button": {
    "$pressed_button_name": "button.menu_network_world_item",
    "bindings": [
      {
        "binding_type": "collection_details",
        "binding_collection_name": "$collection_name"
      },
      {
        "binding_name": "#network_world_button_enabled",
        "binding_name_override": "#enabled",
        "binding_collection_name": "$collection_name",
        "binding_type": "collection"
      }
    ],
    "$button_text": "selectServer.select"
  },

  "network_servers_button@play_mcjava.grid_button": {
    "$pressed_button_name": "button.menu_network_world_item",
    "bindings": [
      {
        "binding_type": "collection_details",
        "binding_collection_name": "$collection_name"
      },
      {
        "binding_name": "#network_world_button_enabled",
        "binding_name_override": "#enabled",
        "binding_collection_name": "$collection_name",
        "binding_type": "collection"
      }
    ],
    "$button_text": "selectServer.select"
  },

  "lan_servers_button@multiplayer_mcjava.network_servers_button": {},

  "friends_servers_button@multiplayer_mcjava.network_servers_button": {},

  "cross_platform_servers_button@multiplayer_mcjava.network_servers_button": {},

  /* Server Names */

  "featured_servers_name_content": {
    "type": "stack_panel",
    "orientation": "horizontal",
    "size": [ "100%", "10px" ],
    "controls": [
      {
        "featured_servers_name_content@play.world_list_text": {
          "text": "#third_party_server_name",
          "localize": false,
          "color": "white",
          "size": [ "default", "default" ],
          "anchor_from": "top_left",
          "anchor_to": "top_left",
          "bindings": [
            {
              "binding_name": "#third_party_server_name",
              "binding_type": "collection",
              "binding_collection_name": "$collection_name"
            }
          ]
        }
      },
      { "fill@common_mcjava.hfill": {} },
      { "ping_rate@multiplayer_mcjava.ping_rate_panel": {} },
      { "ping_loading@multiplayer_mcjava.ping_loading_panel": {} }
    ]
  },

  "more_servers_name_content": {
    "type": "stack_panel",
    "orientation": "horizontal",
    "size": [ "100%", "10px" ],
    "controls": [
      {
        "more_servers_name_content@play.world_list_text": {
          "text": "#network_world_header",
          "color": "white",
          "shadow": false,
          "bindings": [
            {
              "binding_name": "#network_world_header",
              "binding_type": "collection",
              "binding_collection_name": "$collection_name"
            }
          ]
        }
      },
      { "fill@common_mcjava.hfill": {} },
      { "ping_rate@multiplayer_mcjava.ping_rate_panel": {} },
      { "ping_loading@multiplayer_mcjava.ping_loading_panel": {} }
    ]
  },

  "network_servers_name_content": {
    "type": "stack_panel",
    "orientation": "horizontal",
    "size": [ "100%", "10px" ],
    "controls": [
      {
        "more_servers_name_content@play.world_list_text": {
          "text": "#network_world_header",
          "color": "white",
          "shadow": false,
          "bindings": [
            {
              "binding_name": "#network_world_header",
              "binding_type": "collection",
              "binding_collection_name": "$collection_name"
            }
          ]
        }
      },
      { "fill@common_mcjava.hfill": {} },
      { "ping_rate@multiplayer_mcjava.ping_rate_panel": {} },
      { "ping_loading@multiplayer_mcjava.ping_loading_panel": {} }
    ]
  },

  "lan_servers_name_content@network_servers_name_content": {},

  "friends_servers_name_content@network_servers_name_content": {},

  "cross_platform_servers_name_content@network_servers_name_content": {},

  /* Server Descriptions */

  "featured_servers_description_content": {
    "layer": 4,
    "type": "panel",
    "size": ["100%", 21],
    "controls": [
      {
        "network_world_details@play.world_list_text": {
          "text": "#third_party_server_message",
          "localize": false,
          "color": [0.50, 0.50, 0.50],
          "size": [ "100%", "100%" ],
          "anchor_from": "top_left",
          "anchor_to": "top_left",
          "bindings": [
            {
              "binding_name": "#third_party_server_message",
              "binding_type": "collection",
              "binding_collection_name": "$collection_name"
            }
          ]
        }
      }
    ]
  },

  "more_servers_description_content": {
    "layer": 4,
    "type": "panel",
    "size": ["100%", 21],
    "controls": [
      {
        "network_world_details@play.world_list_text": {
          "text": "#network_world_details",
          "color": [0.50, 0.50, 0.50],
          "size": [ "100%", "100%" ],
          "anchor_from": "top_left",
          "anchor_to": "top_left",
          "bindings": [
            {
              "binding_name": "#network_world_details",
              "binding_type": "collection",
              "binding_collection_name": "$collection_name"
            }
          ]
        }
      }
    ]
  },

  "network_servers_description_content": {
    "layer": 4,
    "type": "panel",
    "size": ["100%", 21],
    "controls": [
      {
        "network_world_details@play.world_list_text": {
          "text": "#network_world_details",
          "color": [0.50, 0.50, 0.50],
          "size": [ "100%", "100%" ],
          "anchor_from": "top_left",
          "anchor_to": "top_left",
          "bindings": [
            {
              "binding_name": "#network_world_details",
              "binding_type": "collection",
              "binding_collection_name": "$collection_name"
            }
          ]
        }
      }
    ]
  },

  "lan_servers_description_content@network_servers_description_content": {},

  "friends_servers_description_content@network_servers_description_content": {},

  "cross_platform_servers_description_content@network_servers_description_content": {},

  "ping_rate_panel": {
    "type": "stack_panel",
    "size": ["100%c", 10],
    "orientation": "horizontal",
    "controls": [
      {
        "player_count": {
          "type": "label",
          "size": [ "default", 10 ],
          "text": "#server_player_count",
          "color": [0.66, 0.66, 0.66],
          "bindings": [
            {
              "binding_type": "collection",
              "binding_name": "#server_player_count",
              "binding_collection_name": "$collection_name"
            }
          ]
        }
      },
      {
        "padding_0": {
          "type": "panel",
          "size": [ 2, "100%" ]
        }
      },
      {
        "ping_rate_icon@multiplayer_mcjava.ping_rate_icon": {}
      }
    ],
    "bindings": [
      {
        "binding_type": "collection",
        "binding_name": "(#is_network_available_and_ping_not_loading)",
        "binding_name_override": "#visible",
        "binding_collection_name": "third_party_server_network_worlds"
      }
    ]
  },
  
  "ping_loading_panel": {
    "type": "stack_panel",
    "size": ["100%c", 10],
    "orientation": "horizontal",
    "controls": [
      {
        "ping_rate_icon@multiplayer_mcjava.ping_loading_icon": {}
      }
    ],
    "bindings": [
      {
        "binding_type": "collection",
        "binding_name": "#is_network_available_and_ping_loading",
        "binding_name_override": "#visible",
        "binding_collection_name": "third_party_server_network_worlds"
      }
    ]
  },

  "ping_rate_icon": {
    "type": "image",
    "size": [ 10, 8 ],
    "bindings": [
      {
        "binding_name": "#texture_name",
        "binding_name_override": "#texture",
        "binding_type": "collection",
        "binding_collection_name": "$collection_name"
      }
    ]
  },

  "scanning_for_servers_panel": {
    "type": "panel",
    "size": ["100%", 46],
    "layer": 10,
    "controls": [
      {
        "warning": {
          "type": "label",
          "color": [1, 1, 1],
          "text": "Scanning for games on your local network",
          "layer": 20,
          "anchor_from": "top_middle",
          "anchor_to": "top_middle",
          "offset": [ 0, 14 ]
        }
      },
      { "@scanning_img": {} }
    ]
  },

  "scanning_img": {
    "type": "image",
    "texture": "src/assets/img/scanning",
    "uv": "@multiplayer_mcjava.scanning_animation",
    "uv_size": [29, 9],
    "size": [ 29, 9 ],
    "anchor_from": "top_middle",
    "anchor_to": "top_middle",
    "offset": [ 0, 23 ]
  },

  "scanning_animation": {
    "anim_type": "flip_book",
    "initial_uv": [ 0, 0 ],
    "frame_count": 3,
    "frame_step": 64,
    "fps": 5,
    "reversible": true,
    "easing": "linear"
  },

  "ping_loading_icon": {
    "type": "image",
    "texture": "assets/minecraft/textures/gui/icons",
    "$pl_duration|default": 0.30,
    "uv": "@multiplayer_mcjava.anim_wf_ping_image_1",
    "uv_size": [ 10, 8 ],
    "size": [ 10, 8 ],
    "layer": 2
  },

  "anim_wf_ping_image_1": {
    "anim_type": "uv",
    "from": [ 10, 176 ],
    "to": [ 10, 176 ],
    "duration": "$pl_duration",
    "next": "@multiplayer_mcjava.anim_wf_ping_image_2"
  },

  "anim_wf_ping_image_2": {
    "anim_type": "uv",
    "from": [ 10, 176 ],
    "to": [ 10, 184 ],
    "duration": 0,
    "next": "@multiplayer_mcjava.anim_wf_ping_image_3"
  },

  "anim_wf_ping_image_3": {
    "anim_type": "uv",
    "from": [ 10, 184 ],
    "to": [ 10, 184 ],
    "duration": "$pl_duration",
    "next": "@multiplayer_mcjava.anim_wf_ping_image_4"
  },

  "anim_wf_ping_image_4": {
    "anim_type": "uv",
    "from": [ 10, 184 ],
    "to": [ 10, 192 ],
    "duration": 0,
    "next": "@multiplayer_mcjava.anim_wf_ping_image_5"
  },

  "anim_wf_ping_image_5": {
    "anim_type": "uv",
    "from": [ 10, 192 ],
    "to": [ 10, 192 ],
    "duration": "$pl_duration",
    "next": "@multiplayer_mcjava.anim_wf_ping_image_6"
  },

  "anim_wf_ping_image_6": {
    "anim_type": "uv",
    "from": [ 10, 192 ],
    "to": [ 10, 200 ],
    "duration": 0,
    "next": "@multiplayer_mcjava.anim_wf_ping_image_7"
  },

  "anim_wf_ping_image_7": {
    "anim_type": "uv",
    "from": [ 10, 200 ],
    "to": [ 10, 200 ],
    "duration": "$pl_duration",
    "next": "@multiplayer_mcjava.anim_wf_ping_image_8"
  },

  "anim_wf_ping_image_8": {
    "anim_type": "uv",
    "from": [ 10, 200 ],
    "to": [ 10, 208 ],
    "duration": 0,
    "next": "@multiplayer_mcjava.anim_wf_ping_image_9"
  },

  "anim_wf_ping_image_9": {
    "anim_type": "uv",
    "from": [ 10, 208 ],
    "to": [ 10, 208 ],
    "duration": "$pl_duration",
    "next": "@multiplayer_mcjava.anim_wf_ping_image_10"
  },

  "anim_wf_ping_image_10": {
    "anim_type": "uv",
    "from": [ 10, 208 ],
    "to": [ 10, 176 ],
    "duration": 0,
    "next": "@multiplayer_mcjava.anim_wf_ping_image_1"
  },
  
  "join_selected_server_button_locked@common_mcjava.locked": {

    "size": [100, 20],
    "anchor_from": "top_left",
    "anchor_to": "top_left",
    "$button_text": "selectServer.select"
  },

  "add_friend_button_locked@common_mcjava.locked": {
    "size": [100, 20],
    "anchor_from": "top_middle",
    "anchor_to": "top_middle",
    "$button_text": "networkWorld.add_friend"
  },

  "edit_selected_server_button_locked@common_mcjava.locked": {
    "size": [150, 20],
    "anchor_from": "bottom_left",
    "anchor_to": "bottom_left",
    "$button_text": "selectServer.edit"
  },

  "add_server_button@button_mcjava.button": {
    "$pressed_button_name": "button.menu_network_add_external",
    "$button_text": "selectServer.add",
    "size": [100, 20],
    "anchor_from": "top_right",
    "anchor_to": "top_right",
    "$button_focus_precedence": 11,
    "bindings": [
      {
        "binding_name": "#cross_platform_enabled",
        "binding_name_override": "#enabled"
      }
    ]
  },

  "add_friend_button@button_mcjava.button": {
    "layer": 2,
    "$pressed_button_name": "button.menu_network_add_friend",
    "$button_text": "networkWorld.add_friend",
    "$button_focus_precedence": 10,
    "size": [100, 20],
    "anchor_from": "top_middle",
    "anchor_to": "top_middle",
    "bindings": [
      {
        "binding_name": "#add_friend_button_visible",
        "binding_name_override": "#visible",
        "binding_type": "global"
      },
      {
        "binding_name": "#cross_platform_enabled",
        "binding_name_override": "#enabled"
      }
    ]
  },

  /* Selection */

  "featured_server_selection_panel@play_mcjava.for_selection_more_less_panel": {
    "$for_src": "assets/minecraft/textures/gui/server_selection",
    "$for_name": "featured_servers",
    "$collection_name": "third_party_server_network_worlds",
    "$double_mapping_id": "button.connect_to_third_party_server"
  },
  
  "more_server_selection_panel@play_mcjava.for_selection_more_less_panel": {
    "$for_src": "assets/minecraft/textures/gui/server_selection",
