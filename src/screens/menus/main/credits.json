{
  "namespace": "credits_mcjava",

  "credits_screen_content_panel": {
    "type": "panel",
    "controls": [
      { "bg@end_poema_mcjava.dirt_background": { "layer": 0 } },
      {
        "vignette_renderer": {
          "type": "custom",
          "renderer": "vignette_renderer",
          "layer": 2,
          "property_bag": {
            "opacity_override": 0.0
          }
        }
      },
      { "scroll@credits_mcjava.scroll": {} },
      { "exit@credits_mcjava.exit_panel": {} }
    ]
  },

  "scroll@scrolling_mcjava.scrolling_panel": {
    "$scroll_track_image_control": "common.empty_panel",
    "size": [ "100%", "100%"],
    "layer": 10,
    "$scroll_bar_offset": [ "50% -6px", 0 ],
    "$scroll_box_visible": false,
    "$scrolling_content": "credits_mcjava.scroll_content"
  },

  /* Content */

  "exit_panel": {
    "type": "panel",
    "layer": 80,
    "controls": [
      {
        "exit_button@toggle_mcjava.toggle_button": {
          "size": [50, 20],
          "anchor_from": "bottom_right",
          "anchor_to": "bottom_right",
          "$toggle_button_text": "Exit",
          "offset": [ -8, -8 ],
          "$cancel_mapping": true,
          "$toggle_index": 0,
          "$toggle_button_name": "credits",
          "$toggle_control_name": "credits_view",
          "$alpha": "@credits_mcjava.anim_exit_btn2"
        }
      }
    ]
  },

  "anim_wait1": {
    "anim_type": "wait",
    "duration": 10,
    "next": "@credits_mcjava.anim_exit_btn1"
  },

  "anim_exit_btn1": {
    "anim_type": "alpha",
    "from": 0,
    "to": 1,
    "duration": 0.1,
    "next": "@credits_mcjava.anim_wait2"
  },

  "anim_wait2": {
    "anim_type": "wait",
    "duration": 10,
    "next": "@credits_mcjava.anim_exit_btn2"
  },

  "anim_exit_btn2": {
    "anim_type": "alpha",
    "from": 1,
    "to": 0,
    "duration": 0.1,
    "next": "@credits_mcjava.anim_wait1"
  },

  "line": {
    "type": "label",
    "size": ["100%", "default"],
    "shadow": true
  },

  "line_break": {
    "type": "label",
    "size": ["100%", "default"],
    "text": "\n\n\n\n\n"
  },

  "line_minbreak": {
    "type": "label",
    "size": ["100%", "default"],
    "text": "\n\n"
  },

  "scroll_content": {
    "type": "stack_panel",
    "size": ["65%", "100%c" ],
    "anchor_from": "top_middle",
    "anchor_to": "top_middle",
    "$ignore_initial|default": false,
    "controls": [
      { "space@common_mcjava.vspace": { "ignored": "$ignore_initial", "size": [0, 148] } },
