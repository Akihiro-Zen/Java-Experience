{
  "namespace": "enchanting_table_mcjava",

  "enchanting_table_screen_content_panel@container_mcjava.base_panel": {
    "$container_bg": "enchanting_table_mcjava.enchanting_table_container_bg",
    "$specified_content": "enchanting_table_mcjava.enchanting_table_content_panel",
    "$texture_src": "assets/minecraft/textures/gui/container/enchanting_table"
  },

  "enchanting_table_content_panel": {
    "type": "panel",
    "controls": [
      { "enchant_label_panel@enchanting_table_mcjava.enchant_label_panel": {} },
      { "enchantments_grid_panel@enchanting_table_mcjava.enchantments_grid_panel": {} }
    ]
  },

  "enchant_label_panel": {
    "type": "panel",
    "size": [60, 61],
    "anchor_from": "top_left",
    "anchor_to": "top_left",
    "offset": [3, 3],
    "controls": [
      { "title@enchanting_table_mcjava.enchant_label": {} },
      { "enchanting_book@enchanting_table_mcjava.enchanting_book_panel": {} },
      { "item@enchanting_table_mcjava.item_grid": {} },
      { "lapis@enchanting_table_mcjava.lapis_grid": {} }
    ]
  },

  "enchantments_grid_panel": {
    "type": "panel",
    "anchor_from": "top_left",
    "anchor_to": "top_left",
    "offset": [60, 14],
    "size": [108, 57],
    "controls": [
      {
        "dust_panel@enchanting_table_mcjava.dust_panel": {
          "anchor_from": "top_left",
          "anchor_to": "top_left",
          "layer": 10
        }
      },
      {
        "enchantments_grid": {
          "type": "grid",
          "collection_name": "#enchant_buttons",
          "size": [108, 57],
          "layer": 2,
          "anchor_from": "center",
          "anchor_to": "center",
          "grid_dimensions": [ 1, 3 ],
          "grid_item_template": "enchanting_table_mcjava.enchant_button_panel"
        }
      }
    ]
  },

  /* Content */

  /* Images */

  "enchanting_table_container_bg@common_mcjava.image": {
    "size": [176, 166],
    "uv": [0, 0],
    "variables": [
      { "requires": "($enchanting_table_resolution = 256x)", "$1": [176, 166] },
      { "requires": "($enchanting_table_resolution = 512x)", "$1": [352, 332] },
      { "requires": "($enchanting_table_resolution = 1024x)", "$1": [704, 664] },
      { "requires": "($enchanting_table_resolution = 2048x)", "$1": [1408, 1328] },
      { "requires": "($enchanting_table_resolution = 4096x)", "$1": [2816, 2656] }
    ]
  },

  "dust_image@common_mcjava.image": {
    "type": "image",
    "size": [16, 16],
    "anchor_from": "top_left",
    "anchor_to": "top_left",
    "offset": [0, 1],
    "bindings": [
      {
        "binding_name": "$visibility_binding_name",
        "binding_name_override": "#visible",
        "binding_collection_name": "#enchant_buttons",
        "binding_type": "collection"
      }
    ]
  },

  "dust_image_selectable@enchanting_table_mcjava.dust_image": {
    "variables": "$variables",
    "layer": 2,
    "$visibility_binding_name": "#selectable_dust_is_visible"
  },

  "dust_image_unselectable@enchanting_table_mcjava.dust_image": {
    "variables": "$disabled_variables",
    "layer": 1,
    "$visibility_binding_name": "#unselectable_dust_is_visible"
  },

  "base_bg@common_mcjava.image": {
    "size": [108, 19]
  },

  "background_with_hover_text@enchanting_table_mcjava.base_bg": {
    "controls": [
      {
        "hover_text@common.hover_text": {
          "bindings": [
            {
              "binding_name": "#hover_text",
              "binding_collection_name": "#enchant_buttons",
              "binding_type": "collection"
            }
          ]
        }
      }
    ]
  },

  "dark_background@enchanting_table_mcjava.base_bg": {
    "variables": [
      { "requires": "($enchanting_table_resolution = 256x)", "$0": [0, 185], "$1": [108, 19] },
      { "requires": "($enchanting_table_resolution = 512x)", "$0": [0, 370], "$1": [216, 38] },
      { "requires": "($enchanting_table_resolution = 1024x)", "$0": [0, 740], "$1": [432, 76] },
      { "requires": "($enchanting_table_resolution = 2048x)", "$0": [0, 1480], "$1": [864, 152] },
      { "requires": "($enchanting_table_resolution = 4096x)", "$0": [0, 2960], "$1": [1728, 304] }
    ]
  },

  "dark_background_with_hover_text@enchanting_table_mcjava.background_with_hover_text": {
    "variables": [
      { "requires": "($enchanting_table_resolution = 256x)", "$0": [0, 185], "$1": [108, 19] },
      { "requires": "($enchanting_table_resolution = 512x)", "$0": [0, 370], "$1": [216, 38] },
      { "requires": "($enchanting_table_resolution = 1024x)", "$0": [0, 740], "$1": [432, 76] },
      { "requires": "($enchanting_table_resolution = 2048x)", "$0": [0, 1480], "$1": [864, 152] },
      { "requires": "($enchanting_table_resolution = 4096x)", "$0": [0, 2960], "$1": [1728, 304] }
    ]
  },

  "active_background@enchanting_table_mcjava.base_bg": {
    "variables": [
      { "requires": "($enchanting_table_resolution = 256x)", "$0": [0, 166], "$1": [108, 19] },
      { "requires": "($enchanting_table_resolution = 512x)", "$0": [0, 332], "$1": [216, 38] },
      { "requires": "($enchanting_table_resolution = 1024x)", "$0": [0, 664], "$1": [432, 76] },
      { "requires": "($enchanting_table_resolution = 2048x)", "$0": [0, 1328], "$1": [864, 152] },
      { "requires": "($enchanting_table_resolution = 4096x)", "$0": [0, 2656], "$1": [1728, 304] }
    ]
  },

  "active_background_with_hover_text@enchanting_table_mcjava.background_with_hover_text": {
    "variables": [
      { "requires": "($enchanting_table_resolution = 256x)", "$0": [0, 204], "$1": [108, 19] },
      { "requires": "($enchanting_table_resolution = 512x)", "$0": [0, 408], "$1": [216, 38] },
      { "requires": "($enchanting_table_resolution = 1024x)", "$0": [0, 816], "$1": [432, 76] },
      { "requires": "($enchanting_table_resolution = 2048x)", "$0": [0, 1632], "$1": [864, 152] },
      { "requires": "($enchanting_table_resolution = 4096x)", "$0": [0, 3264], "$1": [1728, 304] }
    ]
  },

  /* #Ended */

  "enchant_label": {
    "type": "label",
    "offset": [ -2, 1 ],
    "anchor_from": "top_middle",
    "anchor_to": "top_middle",
    "text": "container.enchant",
    "color": [0.25, 0.25, 0.25],
    "layer": 10
  },

  "enchanting_book_panel": {
    "type": "panel",
    "offset": [ 0, -4.5 ],
    "size": [ 40, 40 ],
    "anchor_from": "center",
    "anchor_to": "center",
    "controls": [
      {
        "enchanting_book": {
          "type": "custom",
          "renderer": "enchanting_book_renderer",
          "layer": 13,
          "bindings": [
            {
              "binding_name": "#open",
              "binding_type": "global"
            }
          ]
        }
      }
    ]
  },

  "item_grid": {
    "type": "grid",
    "grid_dimensions": [ 1, 1 ],
    "anchor_from": "bottom_middle",
    "anchor_to": "bottom_middle",
    "offset": [ -10, 0 ],
    "size": [ 18, 18 ],
    "collection_name": "enchanting_input_items",
    "controls": [
      {
        "item_slot@container_mcjava.container_slot": {
          "grid_position": [ 0, 0 ],
          "$item_collection_name": "enchanting_input_items"
        }
      }
    ]
  },

  "lapis_grid": {
    "type": "grid",
    "grid_dimensions": [ 1, 1 ],
    "anchor_from": "bottom_middle",
    "anchor_to": "bottom_middle",
    "offset": [ 10, 0 ],
    "size": [ 18, 18 ],
    "collection_name": "enchanting_lapis_items",
    "controls": [
      {
        "lapis@container_mcjava.container_slot": {
          "grid_position": [ 0, 0 ],
          "$item_collection_name": "enchanting_lapis_items",
          "$focus_id_binding_type": "none",
          "$focus_id": "enchanting_lapis"
        }
      }
    ]
  },

  "dust_panel": {
    "type": "grid",
    "grid_dimensions": [ 1, 3 ],
    "size": [18, 57],
    "layer": 5,
    "offset": [1, 0],
    "collection_name": "#enchant_buttons",
    "controls": [
      {
        "a@enchanting_table_mcjava.dust_template": {
          "grid_position": [ 0, 0 ],
          "$variables": [
            { "requires": "($enchanting_table_resolution = 256x)", "$0": [0, 223], "$1": [16, 16] },
            { "requires": "($enchanting_table_resolution = 512x)", "$0": [0, 446], "$1": [32, 32] },
            { "requires": "($enchanting_table_resolution = 1024x)", "$0": [0, 892], "$1": [64, 64] },
            { "requires": "($enchanting_table_resolution = 2048x)", "$0": [0, 1784], "$1": [128, 128] },
            { "requires": "($enchanting_table_resolution = 4096x)", "$0": [0, 3568], "$1": [256, 256] }
          ],
          "$disabled_variables": [
            { "requires": "($enchanting_table_resolution = 256x)", "$0": [0, 239], "$1": [16, 16] },
            { "requires": "($enchanting_table_resolution = 512x)", "$0": [0, 478], "$1": [32, 32] },
            { "requires": "($enchanting_table_resolution = 1024x)", "$0": [0, 956], "$1": [64, 64] },
            { "requires": "($enchanting_table_resolution = 2048x)", "$0": [0, 1912], "$1": [128, 128] },
            { "requires": "($enchanting_table_resolution = 4096x)", "$0": [0, 3824], "$1": [256, 256] }
          ]
        }
      },
      {
        "b@enchanting_table_mcjava.dust_template": {
          "grid_position": [ 0, 1 ],
          "$variables": [
            { "requires": "($enchanting_table_resolution = 256x)", "$0": [16, 223], "$1": [16, 16] },
            { "requires": "($enchanting_table_resolution = 512x)", "$0": [32, 446], "$1": [32, 32] },
            { "requires": "($enchanting_table_resolution = 1024x)", "$0": [64, 892], "$1": [64, 64] },
            { "requires": "($enchanting_table_resolution = 2048x)", "$0": [128, 1784], "$1": [128, 128] },
            { "requires": "($enchanting_table_resolution = 4096x)", "$0": [256, 3568], "$1": [256, 256] }
          ],
          "$disabled_variables": [
            { "requires": "($enchanting_table_resolution = 256x)", "$0": [16, 239], "$1": [16, 16] },
            { "requires": "($enchanting_table_resolution = 512x)", "$0": [32, 478], "$1": [32, 32] },
            { "requires": "($enchanting_table_resolution = 1024x)", "$0": [64, 956], "$1": [64, 64] },
            { "requires": "($enchanting_table_resolution = 2048x)", "$0": [128, 1912], "$1": [128, 128] },
            { "requires": "($enchanting_table_resolution = 4096x)", "$0": [256, 3824], "$1": [256, 256] }
          ]
        }
      },
      {
        "c@enchanting_table_mcjava.dust_template": {
          "grid_position": [ 0, 2 ],
          "$variables": [
            { "requires": "($enchanting_table_resolution = 256x)", "$0": [32, 223], "$1": [16, 16] },
            { "requires": "($enchanting_table_resolution = 512x)", "$0": [64, 446], "$1": [32, 32] },
            { "requires": "($enchanting_table_resolution = 1024x)", "$0": [128, 892], "$1": [64, 64] },
            { "requires": "($enchanting_table_resolution = 2048x)", "$0": [256, 1784], "$1": [128, 128] },
            { "requires": "($enchanting_table_resolution = 4096x)", "$0": [512, 3568], "$1": [256, 256] }
          ],
          "$disabled_variables": [
            { "requires": "($enchanting_table_resolution = 256x)", "$0": [32, 239], "$1": [16, 16] },
            { "requires": "($enchanting_table_resolution = 512x)", "$0": [64, 478], "$1": [32, 32] },
            { "requires": "($enchanting_table_resolution = 1024x)", "$0": [128, 956], "$1": [64, 64] },
            { "requires": "($enchanting_table_resolution = 2048x)", "$0": [256, 1912], "$1": [128, 128] },
            { "requires": "($enchanting_table_resolution = 4096x)", "$0": [512, 3824], "$1": [256, 256] }
          ]
        }
      }
    ]
  },

  "dust_template": {
    "type": "panel",
    "size": [18, 19],
    "controls": [
      { "dust_image_selectable@enchanting_table_mcjava.dust_image_selectable": {} },
      { "dust_image_unselectable@enchanting_table_mcjava.dust_image_unselectable": {} }
    ]
  },

  "enchant_button_panel": {
    "type": "panel",
    "size": [108, 19],
    "layer": 7,
    "controls": [
      { "dark_background@enchanting_table_mcjava.dark_background": {} },
      { "unselectable_button@enchanting_table_mcjava.unselectable_button": {} },
      { "selectable_button@enchanting_table_mcjava.selectable_button": {} }
    ]
  },
  
  "unselectable_button@enchanting.base_button": {
    "$visibility_binding_name": "#unselectable_button_visibility",
    "sound_volume": 0.0,
    "layer": 1,
    "button_mappings": [],
    "controls": [
      { "default@enchanting_table_mcjava.dark_background": {} },
      { "hover@enchanting_table_mcjava.dark_background_with_hover_text": {} },
      { "pressed@enchanting_table_mcjava.dark_background": {} },
      { "runes_panel@enchanting_table_mcjava.runes_panel": { "$runes_color": "$enchanting_runes_fail_color" } },
      { "enchant_cost@enchanting_table_mcjava.fail_cost": {} }
    ]
  },

  "selectable_button@enchanting.base_button": {
    "$visibility_binding_name": "#selectable_button_visibility",
    "$focus_override_left": "enchanting_lapis",
    "$focus_override_right": "FOCUS_OVERRIDE_STOP",
    "sound_name": "block.enchantment_table.use",
    "button_mappings": [
      {
        "from_button_id": "button.menu_select",
        "to_button_id": "button.menu_ok",
        "mapping_type": "pressed"
      },
      {
        "from_button_id": "button.menu_ok",
        "to_button_id": "button.menu_ok",
        "mapping_type": "focused"
      }
    ],
    "controls": [
      { "default@enchanting_table_mcjava.active_background": {} },
      { "hover@enchanting_table_mcjava.active_background_with_hover_text": {} },
      { "pressed@enchanting_table_mcjava.dark_background": {} },
      { "runes_panel@enchanting_table_mcjava.runes_panel": { "$runes_color": "$enchanting_runes_fail_color" } },
      { "enchant_cost@enchanting_table_mcjava.success_cost": {} }
    ]
  },

  "runes_panel": {
    "type": "panel",
    "size": [78, 16],
    "anchor_from": "top_left",
    "anchor_to": "top_left",
    "offset": [19, 2],
    "controls": [
      { "@enchant_runes": {} }
    ]
  },

  "enchant_runes": {
    "type": "label",
    "color": "$runes_color",
    "$runes_color|default": "$enchanting_runes_color",
    "shadow": false,
    "layer": 1,
    "anchor_to": "top_left",
    "anchor_from": "top_left",
    "text": "#runes",
    "size": [ 78, 16 ],
    "font_type": "rune",
    "bindings": [
      {
        "binding_collection_name": "#enchant_buttons",
        "binding_name": "#runes",
        "binding_type": "collection"
      }
    ]
  },

  "enchant_cost": {
    "type": "label",
    "color": "$enchanting_cost_success_color",
    "shadow": true,
    "offset": [ -2, -1 ],
    "layer": 1,
    "anchor_from": "bottom_right",
    "anchor_to": "bottom_right",
    "text": "#cost",
    "bindings": [
      {
        "binding_name": "#cost",
        "binding_collection_name": "#enchant_buttons",
        "binding_type": "collection"
      }
    ]
  },

  "success_cost@enchanting_table_mcjava.enchant_cost": {
    "color": "$enchanting_cost_success_color"
  },

  "fail_cost@enchanting_table_mcjava.enchant_cost": {
    "color": [0.25, 0.49, 0.06]
  }
}