  /* Tabs */

  "creative_tab@tab_mcjava.tab": {
    "$tab_name": "creative_tabs",
    "$toggle_focus_enabled": false,
    "$show_hover_text": true,
    "anchor_from": "top_left",
    "$tab_content": "creative_inventory_mcjava.item_renderer",
    "anchor_to": "top_left",
    "size": [28, 32],
    "$texture_src": "assets/minecraft/textures/gui/container/creative_inventory/tabs",
    "controls": [
      {
        "in": {
          "type": "panel",
          "$tab_control_name": "($name + '_view')",
          "$hover_text": "('itemGroup.' + $name)",
          "$bg_checked_src": "('new_creative_inventory_content_mcjava.' + $name + '_checked')",
          "$bg_unchecked_src": "('new_creative_inventory_content_mcjava.' + $name + '_unchecked')",
          "controls": [
            {
              "$tab_control_name@common_mcjava.toggle": {
                /* These states are for passing the button state to the button content */
                "$checked_state|default": false,
                "$unchecked_state|default": false,
                "$checked_hover_state|default": false,
                "$unchecked_hover_state|default": false,
                "$locked_state|default": false,
                "$toggle_property_bag": {
                  "#hover_text": "gui.done"
                },
                "toggle_name": "$tab_name",
                "$tab_type|default": "tab_mcjava.tab_state_panel",
                "$tab_background|default": "tab_mcjava.tab_background_panel",
                "$text_default_color|default": "$button_default_text_color",
                "$text_locked_color|default": "$button_locked_text_color",
                "radio_toggle_group": true,
                "$toggle_tts_name": "accessibility.tab.tts.title",
                "$toggle_tts_enabled_binding_type": "global",
                "controls": [
                  {
                    "unchecked@$tab_type": {
                      "$img_src": "$bg_unchecked_src",
                      "$bg_zindex": -5,
                      "$text_color": "$text_default_color",
                      "$unchecked_state": true,
                      "layer": 1
                    }
                  },
                  {
                    "unchecked_hover@$tab_type": {
                      "$img_src": "$bg_unchecked_src",
                      "$bg_zindex": -5,
                      "$text_color": "$text_default_color",
                      "$unchecked_hover_state": true,
                      "layer": 3
                    }
                  },
                  {
                    "unchecked_locked@$tab_type": {
                      "$img_src": "$bg_locked_src",
                      "$bg_zindex": -5,
                      "$text_color": "$text_locked_color",
                      "$locked_state": true,
                      "layer": 1
                    }
                  },
                  {
                    "unchecked_locked_hover@$tab_type": {
                      "$img_src": "$bg_locked_src",
                      "$bg_zindex": -5,
                      "$text_color": "$text_locked_color",
                      "$locked_state": true,
                      "layer": 1
                    }
                  },
                  {
                    "checked@$tab_type": {
                      "$img_src": "$bg_checked_src",
                      "$text_color": "$text_default_color",
                      "$checked_state": true,
                      "layer": 3
                    }
                  },
                  {
                    "checked_hover@$tab_type": {
                      "$img_src": "$bg_checked_src",
                      "$text_color": "$text_default_color",
                      "$checked_hover_state": true,
                      "layer": 4
                    }
                  },
                  {
                    "checked_locked@$tab_type": {
                      "$img_src": "$bg_locked_src",
                      "$text_color": "$text_locked_color",
                      "$locked_state": true,
                      "layer": 1
                    }
                  },
                  {
                    "checked_locked_hover@$tab_type": {
                      "$img_src": "$bg_locked_src",
                      "$text_color": "$text_locked_color",
                      "$locked_state": true,
                      "layer": 1
                    }
                  }
                ]
              }
            }
          ]
        }
      }
    ],
    "variables": [
      { "requires": "($creative_inventory_resolution = 256x)", "$1": [28, 32] },
      { "requires": "($creative_inventory_resolution = 512x)", "$1": [56, 64] },
      { "requires": "($creative_inventory_resolution = 1024x)","$1": [112, 128] },
      { "requires": "($creative_inventory_resolution = 2048x)", "$1": [224, 256] },
      { "requires": "($creative_inventory_resolution = 4096x)", "$1": [448, 512] }
    ]
  },

