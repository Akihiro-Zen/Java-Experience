{
  "namespace": "creative_inventory_mcjava",

  "creative_inventory_screen_content_panel": {
    "type": "panel",
    "controls": [
      {
        "centered": {
          "type": "panel",
          "size": [ 195, 136 ],
          "anchor_from": "center",
          "anchor_to": "center",
          "layer": 10,
          "controls": [
            { 
              "@creative_inventory_mcjava.tab_item_content_panel": { 
               /*  "$source_name": "building_blocks_view" */
                "$tab_title": "Construction",
                "bindings": [
                  {
                    "binding_type": "view",
                    "source_control_name": "construction_view",
                    "source_property_name": "#toggle_state",
                    "target_property_name": "#visible"
                  }
                ]
              }
            },
            {
              "@creative_inventory_mcjava.tab_item_content_panel": {
                /* "$source_name": "decorative_blocks_view" */
                "$tab_title": "Equipment",
                "bindings": [
                  {
                    "binding_type": "view",
                    "source_control_name": "equipment_view",
                    "source_property_name": "#toggle_state",
                    "target_property_name": "#visible"
                  }
                ]
              } 
            },
            { 
              "@creative_inventory_mcjava.tab_item_content_panel": { 
               /*  "$source_name": "redstone_view"  */
               "$tab_title": "Items",
                "bindings": [
                  {
                    "binding_type": "view",
                    "source_control_name": "items_view",
                    "source_property_name": "#toggle_state",
                    "target_property_name": "#visible"
                  }
                ]
              } 
            },
            { 
              "@creative_inventory_mcjava.tab_item_content_panel": { 
               /*  "$source_name": "transportation_view"  */
               "$tab_title": "Nature",
               "bindings": [
                  {
                    "binding_type": "view",
                    "source_control_name": "nature_view",
                    "source_property_name": "#toggle_state",
                    "target_property_name": "#visible"
                  }
                ]
              } 
            },
            { "@creative_inventory_mcjava.construction_tab": {} },
            { "@creative_inventory_mcjava.equipment_tab": {} },
            { "@creative_inventory_mcjava.items_tab": {} },
            { "@creative_inventory_mcjava.nature_tab": {} },
            { "@creative_inventory_mcjava.survival_inventory_tab": {} },
            { "@creative_inventory_mcjava.search_tab": {} },
            { "@creative_inventory_mcjava.tab_search_items_content_panel": {} },
            { "@creative_inventory_mcjava.tab_survival_inventory_content_panel": {} }
          ]
        }
      },
      { "inventory_take_progress_icon_button@container_mcjava.inventory_take_progress_icon_button": {} },
      { "inventory_selected_icon_button@container_mcjava.inventory_selected_icon_button": {} },
      { "hold_icon@container_mcjava.inventory_take_progress_icon_button": {} },
      {
        "controller_gamepad_helpers_stack_panel": {
          "type": "stack_panel",
          "anchor_from": "bottom_middle",
          "anchor_to": "bottom_middle",
          "controls": [
            { "container_gamepad_helpers_second_row@crafting.container_gamepad_helpers_second_row": {} },
            {
              "container_gamepad_helpers@common.container_gamepad_helpers": {
                "$helper_x_control": "crafting.inventory_x_gamepad_helper",
                "$helper_y_control": "crafting.inventory_y_gamepad_helper",
                "$helper_a_control": "crafting.inventory_a_gamepad_helper"
              }
            }
          ],
          "bindings": [
            {
              "binding_name": "#gamepad_helper_visible",
              "binding_name_override": "#visible"
            }
          ]
        }
      },
      {
        "game_tip@game_tip.game_tip_panel": {
          "layer": 100,
          "$offset|default": [ 0, 110 ],
          "variables": [
            {
              "requires": "$game_pad",
              "$offset": [ 0, "38%" ]
            }
          ],
          "offset": "$offset"
        }
      }
