{
  "namespace": "progress",

  "modal_progress_screen@progress_screen": {
    "$screen_content": "world_progress_mcjava.modal_screen_content_panel"
  },

  "delete_progress_screen@modal_progress_screen": {
    "$screen_content": "world_progress_mcjava.delete_progress_screen_content_panel",
    "$screen_background": "common_mcjava.dirt_background"
  },

  "realms_loading_progress_screen@progress_screen": {
    "$screen_content": "world_progress_mcjava.realms_progress_screen_content_panel",
    "$screen_background": "common_mcjava.dirt_background"
  },

  "world_loading_progress_screen@progress_screen": {
    "$screen_content": "world_progress_mcjava.world_progress_screen_content_panel",
    "$screen_background": "common_mcjava.dirt_background"
  },

  "overworld_loading_progress_screen@world_loading_progress_screen": {
    "$screen_content": "dimension_progress_mcjava.overworld_progress_screen_content_panel",
    "$screen_background": "common_mcjava.dirt_background"
  },

  "nether_loading_progress_screen@world_loading_progress_screen": {
    "$screen_content": "dimension_progress_mcjava.nether_progress_screen_content_panel",
    "$screen_background": "common_mcjava.dirt_background"
  },

  "theend_loading_progress_screen@world_loading_progress_screen": {
    "$screen_content": "dimension_progress_mcjava.the_end_progress_screen_content_panel",
    "$screen_background": "common_mcjava.dirt_background"
  },

  "world_saving_progress_screen@progress_screen": {
    "$screen_content": "world_progress_mcjava.world_save_progress_screen_content_panel",
    "$screen_background": "common_mcjava.dirt_background"
  },

  "world_convert_modal_progress_screen@progress.progress_screen": {
    "$screen_content": "world_progress_mcjava.world_convert_progress_screen_content_panel",
    "$screen_background": "common_mcjava.dirt_background"
  },

  "progress_screen@common_mcjava.base_screen": {
    "force_render_below": "$force_render_below",
    "button_mappings": [
      {
        "from_button_id": "button.menu_cancel",
        "to_button_id": "button.menu_exit",
        "mapping_type": "global"
      }
    ]
  }
}