{
  "namespace": "store_search",

  "store_search_screen@common_store.store_base_screen": {
    "button_mappings": [
      {
        "from_button_id": "button.menu_cancel",
        "to_button_id": "button.menu_exit",
        "mapping_type": "global"
      },
      {
        "from_button_id": "button.controller_secondary_select",
        "to_button_id": "button.close_result_panel",
        "mapping_type": "global",
        "ignored": "$ignore_3rd_party_servers"
      },
      {
        "from_button_id": "button.controller_select",
        "to_button_id": "button.coin_wallet",
        "mapping_type": "global"
      }
    ],
    "$default_text_color": [ 0.3, 0.3, 0.3 ],
    "$screen_content": "store_search_mcjava.store_search_screen_content_panel",
    "$screen_background": "common_mcjava.dirt_background"
  }
}